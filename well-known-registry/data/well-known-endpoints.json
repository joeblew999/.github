    "change-password": {
      "path": "/.well-known/change-password",
      "category": "authentication",
      "method": "GET",
      "response_type": "redirect",
      "status_code": 302,
      "description": "Browser password change redirect endpoint",
      "purpose": "Browsers redirect here when user wants to change password",
      "authority_level": "official",
      "verification_status": "verified",
      "first_collected": "2025-01-04T12:00:00Z",
      "last_verified": "2025-01-04T12:00:00Z",
      "browser_support": [
        "chrome",
        "safari", 
        "edge",
        "firefox"
      ],
      "specifications": [
        "RFC 8615"
      ],
      "sources": [
        "https://www.iana.org/assignments/well-known-uris/",
        "https://github.com/moul/awesome-well-known"
      ],
      "implementation": {
        "go": "http.Redirect(w, r, \"/account/password\", http.StatusFound)",
        "nginx": "return 302 https://yourdomain.com/account/password;",
        "apache": "Redirect 302 /.well-known/change-password /account/password"
      },
      "example": {
        "request": "GET /.well-known/change-password HTTP/1.1",
        "response": "HTTP/1.1 302 Found\nLocation: https://yourdomain.com/account/password"
      },
      "security_considerations": [
        "Must use HTTPS in production",
        "Should validate referrer to prevent abuse"
      ]
    },
    "webauthn": {
      "path": "/.well-known/webauthn",
      "category": "authentication", 
      "method": "GET",
      "response_type": "json",
      "content_type": "application/json",
      "description": "WebAuthn/Passkey configuration discovery",
      "purpose": "Passkey configuration for browsers and authenticators",
      "authority_level": "official",
      "verification_status": "verified",
      "first_collected": "2025-01-04T12:00:00Z",
      "last_verified": "2025-01-04T12:00:00Z",
      "browser_support": [
        "chrome",
        "safari",
        "firefox",
        "edge"
      ],
      "specifications": [
        "W3C WebAuthn Level 2",
        "RFC 8615"
      ],
      "sources": [
        "https://w3c.github.io/webauthn/",
        "https://github.com/moul/awesome-well-known"
      ],
      "implementation": {
        "go": "w.Header().Set(\"Content-Type\", \"application/json\")\nw.Write([]byte(`{\"origins\":[\"https://yourdomain.com\"]}`))",
        "nginx": "add_header Content-Type application/json;\nreturn 200 '{\"origins\":[\"https://yourdomain.com\"]}';",
        "apache": "Header set Content-Type \"application/json\"\necho '{\"origins\":[\"https://yourdomain.com\"]}'"
      },
      "example": {
        "request": "GET /.well-known/webauthn HTTP/1.1",
        "response": "HTTP/1.1 200 OK\nContent-Type: application/json\n\n{\"origins\":[\"https://yourdomain.com\"],\"features\":{\"conditional_ui\":true}}"
      },
      "schema": {
        "type": "object",
        "properties": {
          "origins": {
            "type": "array",
            "items": {"type": "string"}
          },
          "features": {
            "type": "object",
            "properties": {
              "conditional_ui": {"type": "boolean"},
              "conditional_get": {"type": "boolean"}
            }
          }
        }
      },
      "security_considerations": [
        "Origins must match actual domain",
        "HTTPS required for WebAuthn",
        "Validate JSON schema"
      ]
    },
    "security-txt": {
      "path": "/.well-known/security.txt",
      "category": "security",
      "method": "GET", 
      "response_type": "text",
      "content_type": "text/plain",
      "description": "Security contact and vulnerability disclosure information",
      "purpose": "How security researchers can contact you",
      "authority_level": "official",
      "verification_status": "verified",
      "first_collected": "2025-01-04T12:00:00Z",
      "last_verified": "2025-01-04T12:00:00Z",
      "browser_support": [
        "universal"
      ],
      "specifications": [
        "RFC 9116",
        "RFC 8615"
      ],
      "sources": [
        "https://securitytxt.org/",
        "https://www.iana.org/assignments/well-known-uris/",
        "https://github.com/moul/awesome-well-known"
      ],
      "implementation": {
        "go": "w.Header().Set(\"Content-Type\", \"text/plain\")\nw.Write([]byte(\"Contact: mailto:security@yourdomain.com\\nExpires: 2025-12-31T23:59:59.000Z\\n\"))",
        "nginx": "add_header Content-Type text/plain;\nreturn 200 'Contact: mailto:security@yourdomain.com\nExpires: 2025-12-31T23:59:59.000Z';",
        "apache": "Header set Content-Type \"text/plain\"\necho 'Contact: mailto:security@yourdomain.com'"
      },
      "example": {
        "request": "GET /.well-known/security.txt HTTP/1.1",
        "response": "HTTP/1.1 200 OK\nContent-Type: text/plain\n\nContact: mailto:security@yourdomain.com\nExpires: 2025-12-31T23:59:59.000Z\nPreferred-Languages: en"
      },
      "required_fields": [
        "Contact",
        "Expires"
      ],
      "optional_fields": [
        "Acknowledgments",
        "Preferred-Languages",
        "Canonical",
        "Policy",
        "Hiring",
        "Encryption"
      ],
      "security_considerations": [
        "Must include Expires field",
        "Contact must be valid",
        "Should be served over HTTPS",
        "Consider PGP encryption for sensitive disclosures"
      ]
    }
